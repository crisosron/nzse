{
  "kind": "collectionType",
  "collectionName": "general_pages",
  "info": {
    "singularName": "general-page",
    "pluralName": "general-pages",
    "displayName": "General Page",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "Patients",
        "Professionals",
        "Root"
      ],
      "required": true,
      "default": "Root"
    },
    "membersOnly": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "blocks": {
      "type": "dynamiczone",
      "components": [
        "content-blocks.button-block",
        "content-blocks.flipbook-block",
        "content-blocks.image-block",
        "content-blocks.text-block",
        "content-blocks.text-with-image-block",
        "content-blocks.card-block-list"
      ],
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "other-blocks.seo-block"
    }
  }
}
